{
  "name": "tomo-cli",
  "version": "1.5.3",
  "description": "A friendly command line tool designed to help create sustainable software using web technology",
  "license": "MIT",
  "main": "dist/api.js",
  "bin": {
    "tomo": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhwohlgemuth/tomo-cli.git"
  },
  "author": "Jason Wohlgemuth <jhwohlgemuth@gmail.com> (omaha.js.org)",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "copy:assets": "cpy '**/templates/*.(js|json|re|html|hbs|css|md|png|ico|otf|svg|woff|woff2|ttf|eot|cert|key)' '**/[.]*' '../dist/' --cwd=src --parents --recursive",
    "dev": "stmux [ \"npm run build:watch\" : \"npm run lint:watch\" ]",
    "prebuild": "npm run build:clean",
    "build": "babel src --out-dir dist",
    "postbuild": "npm run copy:assets",
    "build:clean": "rm -frd ./dist",
    "build:watch": "watch 'npm run build' ./src",
    "build:docs": "esdoc -c ./esdoc.conf.json",
    "test": "jest .*.test.js --coverage",
    "test:e2e": "jest .*.e2e.js",
    "test:ing": "npm test -- --watchAll",
    "open:coverage": "open-cli ./coverage/lcov-report/index.html",
    "open:docs": "open-cli ./docs/index.html",
    "lint": "eslint . -c ./.eslintrc.js --fix",
    "lint:watch": "watch 'npm run lint' ./src",
    "lint:tests": "eslint __tests__/**/*.js -c ./.eslintrc.js --fix --no-ignore",
    "lint:tests:watch": "watch 'npm run lint:tests' ./__tests__",
    "lint:config": "eslint -c ./.eslintrc.js *.js --fix",
    "lint:docs": "eslint src/*.js src/**/*.js --no-eslintrc --rule valid-jsdoc:error --parser babel-eslint --plugin react-hooks",
    "lint:all": "npm-run-all lint lint:tests lint:docs",
    "predocs": "npm run lint:docs",
    "docs": "npm run build:docs",
    "postdocs": "npm run open:docs",
    "preversion": "npm-run-all lint build test",
    "postversion": "git push && git push --tags",
    "prerelease": "npm run preversion",
    "release": "release"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.(e2e|test).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/__tests__/fixtures"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "keywords": [],
  "dependencies": {
    "@babel/runtime": "7.7.7",
    "cardinal": "^2.1.1",
    "chalk": "3.0.0",
    "common-tags": "^1.8.0",
    "conf": "^6.2.0",
    "core-js": "^3.1.4",
    "delay": "4.3.0",
    "execa": "4.0.0",
    "figures": "3.1.0",
    "fs-extra": "8.1.0",
    "get-stdin": "^7.0.0",
    "ink": "2.3.0",
    "ink-big-text": "1.0.1",
    "ink-box": "1.0.0",
    "ink-multi-select": "^1.1.0",
    "ink-select-input": "3.1.2",
    "ink-spinner": "3.0.1",
    "is-online": "^8.2.0",
    "lodash.merge": "^4.6.2",
    "mem-fs": "1.1.3",
    "mem-fs-editor": "6.0.0",
    "meow": "6.0.0",
    "p-queue": "6.2.1",
    "pino": "^5.12.3",
    "pino-pretty": "^3.0.0",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "ramda": "^0.26.1",
    "react": "16.10.2",
    "read-pkg-up": "^7.0.1",
    "semver": "7.1.1",
    "string-similarity": "4.0.1",
    "update-notifier": "4.0.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-export-default-from": "7.7.4",
    "@babel/plugin-proposal-optional-chaining": "7.7.5",
    "@babel/plugin-proposal-pipeline-operator": "7.7.7",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-ramda": "2.0.0",
    "babel-preset-minify": "0.5.1",
    "cpy-cli": "3.0.0",
    "directory-tree": "2.2.4",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-jsx-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "6.8.0",
    "eslint-config-omaha-prime-grade": "11.0.4",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "ink-testing-library": "1.0.2",
    "jest": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "npm-run-all": "4.1.5",
    "open-cli": "5.0.0",
    "release": "6.1.0",
    "rimraf": "3.0.0",
    "sinon": "8.0.2",
    "stmux": "1.8.0",
    "watch": "1.0.2"
  },
  "bugs": {
    "url": "https://github.com/jhwohlgemuth/tomo-cli/issues"
  },
  "homepage": "https://github.com/jhwohlgemuth/tomo-cli#readme"
}
